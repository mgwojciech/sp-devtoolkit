{
    "version": "1.0",
    "manifest_version": 2,
    "name" : "SharePoint Dev Kit",
    "description": "This extensions allows You to run REST API calls using current user digest",

    "permissions": [
        "activeTab",
        "storage"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html",
        "default_title": "Call REST API"
    }
}